{
  "name": "peanut-suite",
  "version": "4.2.39",
  "description": "Peanut Suite - Complete Marketing Toolkit for WordPress",
  "private": true,
  "scripts": {
    "dev": "cd frontend && npm run dev",
    "build": "cd frontend && npm run build",
    "build:production": "cd frontend && npm run build",
    "install:frontend": "cd frontend && npm install",
    "clean": "rm -rf dist peanut-suite.zip",
    "package": "npm run build:production && bash scripts/package.sh",
    "release": "npm run clean && npm run package",
    "bump": "bash scripts/bump-version.sh",
    "bump:patch": "bash scripts/bump-version.sh patch",
    "bump:minor": "bash scripts/bump-version.sh minor",
    "bump:major": "bash scripts/bump-version.sh major",
    "deploy": "rsync -avz --delete --exclude='.git' --exclude='node_modules' --exclude='.env' --exclude='/dist' ./ peanutgraphic:~/public_html/wp-content/plugins/peanut-suite/",
    "release:patch": "bash scripts/release.sh patch",
    "release:minor": "bash scripts/release.sh minor",
    "release:major": "bash scripts/release.sh major"
  },
  "keywords": [
    "wordpress",
    "plugin",
    "utm",
    "marketing",
    "links",
    "crm",
    "analytics"
  ],
  "author": "Peanut Graphic",
  "license": "GPL-2.0-or-later"
}
